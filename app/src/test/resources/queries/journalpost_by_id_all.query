query journalpostById($journalpostId: String!) {
  journalpostById(journalpostId: $journalpostId) {
    journalpostId
    tittel
    journalposttype
    journalstatus
    tema
    eksternReferanseId
    sak {
      fagsakId
      fagsaksystem
      sakstype
    }
    avsender {
      id
      type
      navn
    }
    mottaker {
      id
      type
      navn
    }
    kanal
    relevanteDatoer {
      dato
      datotype
    }
    dokumenter {
      dokumentInfoId
      tittel
      brevkode
      dokumentvarianter {
        variantformat
        filuuid
        filtype
        filstorrelse
        brukerHarTilgang
        code
      }
    }
  }
}